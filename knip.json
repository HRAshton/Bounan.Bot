{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignore": [
    "third-party/**",
    "scripts/**"
  ],
  "workspaces": {
    "packages/app": {
      "entry": "src/handlers/*/handler.ts",
      "ignoreDependencies": [
        "@aws-sdk/client-ssm",
        "@aws-sdk/client-lambda"
      ]
    },
    "packages/infra": {
      "entry": "src/app.ts"
    }
  }
}